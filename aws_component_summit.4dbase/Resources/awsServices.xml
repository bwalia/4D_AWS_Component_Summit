    <aws_service name="ec2">
        <region code="us-east-1"      endpoint="ec2.us-east-1.amazonaws.com"        name="US East (N. Virginia)"/>
        
        <region code="us-west-2"      endpoint="ec2.us-west-2.amazonaws.com"        name="US West (Oregon)"/>
        <region code="us-west-1"      endpoint="ec2.us-west-1.amazonaws.com"        name="US West (N. California)"/>
        
        <region code="eu-west-1"      endpoint="ec2.eu-west-1.amazonaws.com"        name="EU (Ireland)"/>
        <region code="eu-central-1"   endpoint="ec2.eu-central-1.amazonaws.com"     name="EU (Frankfurt)"/>
        
        <region code="ap-southeast-1" endpoint="ec2.ap-southeast-1.amazonaws.com"   name="Asia Pacific (Singapore)"/>
        <region code="ap-northeast-1" endpoint="ec2.ap-northeast-1.amazonaws.com"   name="Asia Pacific (Tokyo)"/>
        
        <region code="ap-southeast-2" endpoint="ec2.ap-southeast-2.amazonaws.com"   name="Asia Pacific (Sydney)"/>
        <region code="ap-northeast-2" endpoint="ec2.ap-northeast-2.amazonaws.com"   name="Asia Pacific (Seoul)"/>
        <region code="ap-south-1"     endpoint="ec2.ap-south-1.amazonaws.com"       name="Asia Pacific (Mumbai)"/>
        
        <region code="sa-east-1"      endpoint="ec2.sa-east-1.amazonaws.com"        name="South America (Sao Paulo)"/>
    </aws_service>

