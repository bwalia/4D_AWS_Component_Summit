        <region code="ap-south-1"     endpoint="ec2.ap-south-1.amazonaws.com"       name="Asia Pacific (Mumbai)"/>
    
        <region code="sa-east-1"      endpoint="rds.sa-east-1.amazonaws.com"        name="South America (Sao Paulo)"/>
