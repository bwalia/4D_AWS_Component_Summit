        <region code="eu-central-1"   endpoint="rds.eu-central-1.amazonaws.com"     name="EU (Frankfurt)"/>
    
        <region code="ap-southeast-1" endpoint="rds.ap-southeast-1.amazonaws.com"   name="Asia Pacific (Singapore)"/>
        <region code="ap-northeast-1" endpoint="rds.ap-northeast-1.amazonaws.com"   name="Asia Pacific (Tokyo)"/>
    
        <region code="ap-southeast-2" endpoint="rds.ap-southeast-2.amazonaws.com"   name="Asia Pacific (Sydney)"/>
